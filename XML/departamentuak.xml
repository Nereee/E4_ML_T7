        <ardura id="A02">
            <izena>Lan zuzendaria</izena>
            <siglak>COO</siglak>
            <deskribapena>
                Plan estrategikoak ezartzeko eta negozioaren eragiketa guztiak aurreikusitako
                denboran eta
                kostuan bere helburuak lortzen dituela ziurtatzeko arduraduna.
            </deskribapena>
        </ardura>
        <ardura id="A03">
            <izena>Zuzendari komertziala</izena>
            <siglak>CSO</siglak>
            <deskribapena>
                Gehieneko kargua da salmenta-sailaren barruan, eta haren funtzio nagusia da
                plangintza
                estrategikoaren helburuekin eta ikuspegiarekin bat datozen salmenta-planak ezartzea.
            </deskribapena>
        </ardura>
        <ardura id="A04">
            <izena>Marketineko zuzendaria</izena>
            <siglak>CMO</siglak>
            <deskribapena>
                Marketinarekin lotuta jarraitu beharreko helburuak eta politikak ezartzea.
                Merkatua eta lehia aztertzea.
            </deskribapena>
        </ardura>
        <ardura id="A05">
            <izena>Giza Baliabideen zuzendaria</izena>
            <siglak>CHRO</siglak>
            <deskribapena>
                Langileen jarduerak eta lan-harremanak planifikatu, zuzendu eta koordinatzea,
                baita konpainia baten politikak eta praktikak ere.
            </deskribapena>
        </ardura>
