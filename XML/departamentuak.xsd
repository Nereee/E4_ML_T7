                                        </xs:element>
                                        <xs:element name="emaila"></xs:element>
                                        <xs:element name="soldata">
                                            <xs:complexType>
                                                <xs:simpleContent>
                                                    <xs:extension base="xs:int">
                                                        <xs:attribute name="moneda"></xs:attribute>
                                                    </xs:extension>
                                                </xs:simpleContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="kontratazioaData"></xs:element>
                                        <xs:element name="ardurak">
                                            <xs:complexType>
                                                <xs:sequence>
